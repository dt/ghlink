{
    "name": "ghlink",
    "displayName": "Link Github Issues",
    "description": "Automatically link mentioned issue numbers (e.g. '#123') to github issues",
    "version": "1.0.2",
    "publisher": "dt",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/dt/ghlink.git"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^7.0.43",
        "@types/parse-git-config": "^2.0.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "parse-git-config": "^2.0.2"
    }
}
